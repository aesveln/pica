<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:camel="http://camel.apache.org/schema/spring"
       xmlns:cxf="http://camel.apache.org/schema/cxf"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
	http://camel.apache.org/schema/cxf http://camel.apache.org/schema/cxf/camel-cxf.xsd
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd        
	http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

    <import resource="classpath:META-INF/cxf/cxf.xml"/>

    <!-- ================================================================= -->
    <!-- SOAP CXF APIS -->
    <!-- ================================================================= -->


    <!-- ================================================================= -->
    <!-- SOAP CXF CLIENTS -->
    <!-- ================================================================= -->

    <bean id="inInterceptor" class="org.apache.cxf.interceptor.LoggingInInterceptor"/>
    <bean id="outInterceptor" class="org.apache.cxf.interceptor.LoggingOutInterceptor"/>

    <cxf:cxfEndpoint id="localPaymentSOAP"
                     address="http://192.168.1.100:8088/mockLocalPaymentSOAP"
                     wsdlURL="http://192.168.1.100:8088/mockLocalPaymentSOAP?WSDL"
                     endpointName="s:LocalPaymentSOAP" serviceName="s:LocalPaymentService"
                     xmlns:s="http://aesveln.com.co/transfor/il/">


        <cxf:inInterceptors>
            <ref bean="inInterceptor"/>
        </cxf:inInterceptors>

        <cxf:outInterceptors>
            <ref bean="outInterceptor"/>
        </cxf:outInterceptors>
    </cxf:cxfEndpoint>



</beans>