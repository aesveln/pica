<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:camel="http://camel.apache.org/schema/spring"

       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="

	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd        
	http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">


    <!-- ================================================================= -->
    <!-- CANONICAL ADAPTER -->
    <!-- ================================================================= -->

    <camel:routeContext id="adaptadores-ach-national-context">

        <camel:route id="adaptador-ach-national-route-execute">
            <camel:from uri="direct:adaptador-ach-national-execute"/>
            <camel:log message="Llamado a ach-national execute."/>
            <camel:to uri="http4://localhost:8080/ACHNationalPayments"/>
			<camel:log message="respuesta servicio ach national system execute. ${body}"/>
        </camel:route>


        <camel:route id="adaptador-ach-national-route-cancel">
            <camel:from uri="direct:adaptador-ach-national-cancel"/>
            <camel:log message="Llamado a ach-national cancel."/>
            <camel:to uri="http4://localhost:8080/ACHNationalPayments"/>
            <camel:log message="respuesta servicio ach national system cancel. ${body}"/>
        </camel:route>


    </camel:routeContext>

</beans>